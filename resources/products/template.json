{
  "products": [
    "{{repeat(50, 70)}}",
    {
      "id": "{{index()}}",
      "name": "{{lorem((1, 4), \"words\")}}",
      "releaseYear": "{{integer(1980, 2017)}}",
      "rating": "{{floating(2.0, 5.0, 2)}}",
      "category":
        "{{random(\"horror\", \"western\", \"action\", \"adventure\", \"thriller\", \"animation\")}}",
      "cast": [
        "{{repeat(2, 4)}}",
        {
          "name": "{{firstName()}} {{surname()}}"
        }
      ],
      "director": "{{firstName()}} {{surname()}}",
      "summary": "{{lorem(1, \"paragraphs\")}}",
      "posterUrl": "http://lorempixel.com/300/500",
      "runtime": "{{integer(58, 180)}} min"
    }
  ],
  "reviews": [
    "{{repeat(200, 280)}}",
    {
      "id": "{{index()}}",
      "productId": "{{integer(0, 70)}}",
      "userId": "{{integer(0, 20)}}",
      "rating": "{{integer(1, 5)}}",
      "title": "{{lorem((1, 8), \"words\")}}",
      "summary": "{{lorem(1, \"paragraphs\")}}"
    }
  ]
}
