{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "outDir": "./dist",
    // "sourceMap": true,
    // "inlineSourceMap": true,
    "strict": true,
    "strictNullChecks": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types",
      "types"
    ],
    "types": [
      "node",
      "stream-replace",
      "cookie-parser"
    ]
  }
}
